#include <iostream>
#include <vector>
using namespace std;
#define ll long long
#define pb push_back
#define ff first
#define mp make_pair
#define no cout << "NO" << '\n'
#define yes cout << "YES" << '\n'
void fast()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}
#define fast() ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0)

void ok()
{
    ll n, last, sl, sum = 0;
    cin >> n;
    vector<int> a(n);

    for (int i = 0; i < n - 2; i++)
    {
        cin >> a[i];
        sum += a[i];
    }
    cin >> sl;
    sl -= sum;
    cin >> last;
    last -= sl;
    cout << last << endl;
}

int main()
{
    fast();
    int t;
    cin >> t;
    while (t--)
    {
        ok();
    }

    return 0;
}
